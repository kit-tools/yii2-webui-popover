{
    "name": "kit-tools/yii2-webui-popover",
    "description": "WebUI Popover description",
    "homepage": "https://github.com/kit-tools/yii2-webui-popover",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii 2",
        "popover",
        "webui-popover",
        "extension",
        "widget"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Zimoglyadov",
            "email": "wppdevelop@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "bower-asset/webui-popover": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "kittools\\webuipopover\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
